<!DOCTYPE html>
<html lang="en">
	<head>
    	<title>Survey Results</title>
        <meta charset="utf-8">
        <link rel="stylesheet" href="~/css/style.css">
	</head>
    <body>
        <!-- This information is displayed ONLY for the Survey Class -->
        <div class="container">  
            <div class="displayInfo">
                @model Survey 
                <h3>Info Submitted by User:</h3>
                <p><span>Name:</span>  @Model.Name</p>
                <p><span>Dojo Location:</span>  @Model.Dojo</p>
                <p><span>Favorite Language:</span>  @Model.Language</p>
                <p><span>Comment:</span>  @Model.Comment</p>
                <a href="/">Return to Form!!!</a>
            </div>
        </div>
        <!-- Information displayed for any other class would required a new Div that displays
            specifically for the Models.className -->
        
    </body>
</html>